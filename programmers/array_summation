# https://programmers.co.kr/learn/courses/30/lessons/12950

def solution(arr1, arr2):
    len_array = len(arr1)
    len_list = len(arr1[0])  
    answer = []
 
    for i in range(len_array):
        lst = []
        for j in range(len_list):
            lst.append(arr1[i][j] + arr2[i][j])
        answer.append(lst)
    return answer
